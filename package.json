{
  "name": "trvcklit",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "description": "travel checklist to help you pack",
  "packageManager": "pnpm@7.6.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lmmmmmm-bb/trvcklit"
  },
  "author": {
    "email": "lmmmmmm12138@gmail.com",
    "name": "_lmmmmmm"
  },
  "bugs": {
    "url": "https://github.com/Lmmmmmm-bb/trvcklit/issues",
    "email": "lmmmmmm12138@gmail.com"
  },
  "maintainers": [
    {
      "name": "_lmmmmmm",
      "email": "lmmmmmm12138@gmail.com"
    }
  ],
  "scripts": {
    "dev": "vite --host & pnpm style:watch",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
    "lint:fix": "eslint --ext .tsx,.ts,.js, --fix ./src --fix",
    "lint:check": "pnpm prettier --check .",
    "lint:rewrite": "pnpm prettier --write .",
    "prepare": "husky install",
    "style:watch": "pnpm typed-scss-modules src --watch --ignore \"**/index.scss\""
  },
  "dependencies": {
    "@douyinfe/semi-icons": "^2.15.1",
    "@douyinfe/semi-ui": "^2.15.1",
    "@supabase/supabase-js": "^1.35.4",
    "framer-motion": "^6.5.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "reset-css": "^5.0.1",
    "sass": "^1.54.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitejs/plugin-react": "^2.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typed-scss-modules": "^6.5.0",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vite-plugin-pwa": "^0.12.3",
    "workbox-window": "^6.5.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{ts,tsx}": "eslint --ext .tsx,.ts,.js,.vue --fix ./src"
  }
}
